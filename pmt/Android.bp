cc_binary { 
    name: "pmt",
    srcs: [
        "pmt.c",
        "docs.c",
        "checkers.c",
        "error.c",
        "listpart.c",
        "tools.c",
        "versioner.c",
    ],
    include_dirs: [
        "src/include",
    ],
    cflags: [
        "-Wall",
        "-g",
        "-O3",
    ],
    target: {
        android: {
            arch: "arm64",
            vendor: true,
        },
    },
    installable: true,
    relative_install_path: "sbin",
}
